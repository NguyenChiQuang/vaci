			&lt;td style=&quot;width:37pt; border-right-width:1pt; border-bottom-width:1pt; border-left-width:1pt; border-style:none solid solid; border-right-color:windowtext; border-bottom-color:windowtext; border-left-color:windowtext; padding:0cm 5.4pt&quot; valign=&quot;top&quot; width=&quot;49&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:11pt&quot;&gt;&lt;span style=&quot;color:black&quot;&gt;4&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:200.5pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt&quot; valign=&quot;top&quot; width=&quot;267&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:11pt&quot;&gt;&lt;span style=&quot;color:black&quot;&gt;Neomycin&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:32.25pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt&quot; valign=&quot;top&quot; width=&quot;43&quot;&gt;&amp;nbsp;&lt;/td&gt;
			&lt;td style=&quot;width:205.1pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt&quot; valign=&quot;top&quot; width=&quot;273&quot;&gt;&amp;nbsp;&lt;/td&gt;
		&lt;/tr&gt;
	&lt;/tbody&gt;
&lt;/table&gt;
