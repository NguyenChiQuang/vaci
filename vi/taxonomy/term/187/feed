			&lt;td style=&quot;width:156.95pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0mm 5.75pt; height:4.5pt&quot; valign=&quot;top&quot; width=&quot;209&quot;&gt;&lt;span style=&quot;font-size:13pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;.VnTime&amp;quot;, sans-serif&quot;&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;Vận tải, lưu kho và thông tin&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:13pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;.VnTime&amp;quot;, sans-serif&quot;&gt;&lt;i&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;Transport, storage and communication&lt;/span&gt;&lt;/span&gt;&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:265.05pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0mm 5.75pt; height:4.5pt&quot; valign=&quot;top&quot; width=&quot;353&quot;&gt;&lt;span style=&quot;font-size:13pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;.VnTime&amp;quot;, sans-serif&quot;&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;49: Tận&lt;span style=&quot;color:black&quot;&gt;&amp;nbsp;tải đường bộ/&amp;nbsp;&lt;i&gt;Land transport&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:13pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;.VnTime&amp;quot;, sans-serif&quot;&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;span style=&quot;color:black&quot;&gt;50: Vận tải đường thủy/&amp;nbsp;&lt;i&gt;Water transport&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
