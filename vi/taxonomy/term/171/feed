			&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;Giá trị độ chia/resolution:&amp;nbsp;0,001 mm&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td width=&quot;113&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;0,8&amp;nbsp;&lt;/span&gt;&lt;span lang=&quot;KO&quot;&gt;&lt;span&gt;μ&lt;/span&gt;&lt;/span&gt;&lt;span lang=&quot;EN-US&quot;&gt;m&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td rowspan=&quot;2&quot; width=&quot;36&quot;&gt;&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;6.&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td rowspan=&quot;2&quot; width=&quot;155&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;VI&quot;&gt;&lt;span&gt;Thước đo sâu&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;
			&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;Depth Gage&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td width=&quot;238&quot;&gt;&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;Đến/&amp;nbsp;to&amp;nbsp;300 mm&lt;/span&gt;&lt;/span&gt;
			&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;Giá trị độ chia/resolution:&amp;nbsp;0,01 mm&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td rowspan=&quot;2&quot; width=&quot;121&quot;&gt;&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;MEB-PP-&lt;/span&gt;&lt;/span&gt;
			&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;003-17 : 2018&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
