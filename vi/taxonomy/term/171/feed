			&lt;span lang=&quot;EN-US&quot;&gt;(0,1 ~ 9999) s&lt;/span&gt;&lt;/td&gt;
			&lt;td width=&quot;143&quot;&gt;&lt;span lang=&quot;EN-US&quot;&gt;DVK.LAB.TM.18&lt;/span&gt;&lt;/td&gt;
