			&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;Power frequency withstand voltage&amp;nbsp;&amp;nbsp;tests&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td width=&quot;125&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;FR&quot;&gt;0 ~ 50 kV&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;
