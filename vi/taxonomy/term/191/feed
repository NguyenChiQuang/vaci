			&lt;td colspan=&quot;2&quot; style=&quot;width:28.46%;height:34px;&quot;&gt;Ngành&lt;br /&gt;
			&lt;em&gt;Category&lt;/em&gt;&lt;/td&gt;
			&lt;td style=&quot;width:52.8%;height:34px;&quot;&gt;Chuyên ngành&lt;br /&gt;
			&lt;em&gt;Subcategory&lt;/em&gt;&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td style=&quot;width:18.74%;height:12px;&quot;&gt;Phục vụ ăn uống&lt;br /&gt;
