			&lt;em&gt;Cluster&lt;/em&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td colspan=&quot;2&quot; style=&quot;width:28.46%;height:34px;&quot;&gt;&lt;span style=&quot;font-size:14px;&quot;&gt;&lt;span style=&quot;font-family:arial,helvetica,sans-serif;&quot;&gt;Ngành&lt;br /&gt;
			&lt;em&gt;Category&lt;/em&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
