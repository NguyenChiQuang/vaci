&lt;span style=&quot;font-size:12px;&quot;&gt;&lt;span style=&quot;font-family:Arial,Helvetica,sans-serif;&quot;&gt;&lt;span style=&quot;caret-color:#000000&quot;&gt;&lt;span style=&quot;color:#000000&quot;&gt;&lt;span style=&quot;font-style:normal&quot;&gt;&lt;span style=&quot;font-variant-caps:normal&quot;&gt;&lt;span style=&quot;font-weight:normal&quot;&gt;&lt;span style=&quot;letter-spacing:normal&quot;&gt;&lt;span style=&quot;text-transform:none&quot;&gt;&lt;span style=&quot;white-space:normal&quot;&gt;&lt;span style=&quot;word-spacing:0px&quot;&gt;&lt;span style=&quot;text-decoration:none&quot;&gt;&lt;b&gt;&lt;span lang=&quot;FR&quot;&gt;NGÀY BAN HÀNH/&amp;nbsp;&lt;/span&gt;&lt;/b&gt;&lt;i&gt;&lt;span lang=&quot;FR&quot;&gt;Issue date&lt;/span&gt;&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
