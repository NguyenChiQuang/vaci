&lt;ul&gt;
	&lt;li&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;ASTM: Hiệp hội Thí nghiệm và Vật liệu Hoa KỳTiêu chuẩn Việt Nam/&amp;nbsp;American Society for Testing and Materials&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;&lt;span&gt;BS EN: Tiêu chuẩn Anh châu Âu/&amp;nbsp;British Standard European Norm./.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
