	&lt;li class=&quot;TableParagraph&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;ASTM: American Society for Testing and Materials&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
	&lt;li class=&quot;TableParagraph&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;EN: Phương pháp thử theo tiêu chuẩn châu Âu/&amp;nbsp;Eropean Norm standard test methods.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/li&gt;
&lt;/ul&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;field field-name-field-dichvu-ngayhieuluc field-type-datetime field-label-above&quot;&gt;&lt;div class=&quot;field-label&quot;&gt;Ngày hiệu lực:&amp;nbsp;&lt;/div&gt;&lt;div class=&quot;field-items&quot;&gt;&lt;div class=&quot;field-item even&quot;&gt;&lt;span class=&quot;date-display-single&quot;&gt;29/11/2020&lt;/span&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;field field-name-field-dichvu-diadiem field-type-text-long field-label-above&quot;&gt;&lt;div class=&quot;field-label&quot;&gt;Địa điểm công nhận:&amp;nbsp;&lt;/div&gt;&lt;div class=&quot;field-items&quot;&gt;&lt;div class=&quot;field-item even&quot;&gt;&lt;span style=&quot;font-size:13pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;.VnTime&amp;quot;, sans-serif&quot;&gt;&lt;span lang=&quot;X-NONE&quot; style=&quot;font-size:11pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;Lô C7, cụm công nghiệp Gián Khẩu, xã Gia Tân, huyện Gia Viễn, tỉnh Ninh Bình&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;field field-name-field-dichvu-file-download field-type-file field-label-hidden&quot;&gt;&lt;div class=&quot;field-items&quot;&gt;&lt;div class=&quot;field-item even&quot;&gt;&lt;span class=&quot;file&quot;&gt;&lt;img class=&quot;file-icon&quot; alt=&quot;PDF icon&quot; title=&quot;application/pdf&quot; src=&quot;/modules/file/icons/application-pdf.png&quot; /&gt; &lt;a href=&quot;http://boa.gov.vn/sites/default/files/846l0419.pdf&quot; type=&quot;application/pdf; length=271525&quot;&gt;846l0419.pdf&lt;/a&gt;&lt;/span&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;field field-name-field-makup-chia-se field-type-markup field-label-hidden&quot;&gt;&lt;div class=&quot;field-items&quot;&gt;&lt;div class=&quot;field-item even&quot;&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;field field-name-field-dichvu-stt-tochuc field-type-number-integer field-label-above&quot;&gt;&lt;div class=&quot;field-label&quot;&gt;Số thứ tự tổ chức:&amp;nbsp;&lt;/div&gt;&lt;div class=&quot;field-items&quot;&gt;&lt;div class=&quot;field-item even&quot;&gt;846&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;</description>
