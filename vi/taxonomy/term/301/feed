			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;i&gt;Count the white blood cell count (WBC)&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:92.1pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;123&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;Tán xạ&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;i&gt;Light scattering method&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:95.1pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;127&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;KT.TB.04&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;(Unicel DxH600)&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr style=&quot;break-inside:avoid; height:3.8pt&quot;&gt;
			&lt;td style=&quot;width:30pt; border-right-width:1pt; border-bottom-width:1pt; border-left-width:1pt; border-style:none solid solid; border-right-color:windowtext; border-bottom-color:windowtext; border-left-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;40&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;4.&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:172.8pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;230&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;Đếm số lượng tiểu cầu (PLT)&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;i&gt;Platelet count (PLT)&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:92.1pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;123&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;Tán xạ&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;i&gt;Light scattering method&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:95.1pt; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;127&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;KT.TB.05&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;(Unicel DxH600)&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
		&lt;/tr&gt;
	&lt;/tbody&gt;
&lt;/table&gt;
&lt;br /&gt;
