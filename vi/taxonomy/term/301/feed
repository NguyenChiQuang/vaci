			&lt;td style=&quot;width:6cm; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;227&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;span style=&quot;line-height:24px&quot;&gt;&lt;span lang=&quot;SV&quot; style=&quot;font-size:11pt&quot;&gt;&lt;span style=&quot;line-height:22px&quot;&gt;Xác định lượng Fibrinogen&amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;span style=&quot;line-height:24px&quot;&gt;&lt;i&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:11pt&quot;&gt;&lt;span style=&quot;line-height:22px&quot;&gt;Determination of Fibrinogen Assay&lt;/span&gt;&lt;/span&gt;&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td style=&quot;width:4cm; border-style:none solid solid none; border-bottom-width:1pt; border-bottom-color:windowtext; border-right-width:1pt; border-right-color:windowtext; padding:0cm 5.4pt; height:3.8pt&quot; width=&quot;151&quot;&gt;&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;span style=&quot;line-height:24px&quot;&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:11pt&quot;&gt;&lt;span style=&quot;line-height:22px&quot;&gt;Phát hiện điểm đông&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;br /&gt;
			&lt;span style=&quot;font-size:12pt&quot;&gt;&lt;span style=&quot;font-family:&amp;quot;Times New Roman&amp;quot;, serif&quot;&gt;&lt;span style=&quot;line-height:24px&quot;&gt;&lt;i&gt;&lt;span lang=&quot;EN-US&quot; style=&quot;font-size:11pt&quot;&gt;&lt;span style=&quot;line-height:22px&quot;&gt;Coagulation point detection&lt;/span&gt;&lt;/span&gt;&lt;/i&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
