		&lt;tr&gt;
			&lt;td width=&quot;40&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span lang=&quot;EN-US&quot;&gt;15. &lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
			&lt;td width=&quot;240&quot;&gt;&lt;span&gt;&lt;span&gt;Định lượng Fibrinogen&lt;/span&gt;&lt;/span&gt;
